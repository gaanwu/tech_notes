      <text:p text:style-name="P8"><text:span text:style-name="T3">git config user.name &quot;&lt;</text:span><text:span text:style-name="T10">你的名稱</text:span><text:span text:style-name="T3">&gt;&quot;</text:span></text:p>
     </table:table-cell>
    </table:table-row>
   </table:table>
